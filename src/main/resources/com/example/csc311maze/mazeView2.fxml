            <Image url="@Images/hardMaze.png" />
         </image>
      </ImageView>
      <Pane fx:id="roboPane" layoutX="1.0" layoutY="363.0" onKeyPressed="#initialize" prefHeight="29.0" prefWidth="27.0">
         <children>
      
            <!--Robot Image-->
            <ImageView fx:id="m2Robot" fitHeight="25.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/robot.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button layoutX="682.0" layoutY="561.0" onAction="#onSolveMazeButtonClick" text="Solve Maze" />
